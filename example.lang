int fibbonacci(int n) {
   if(n == 0){
      return 0;
   } else if(n == 1) {
      return 1;
   } else {
      return (fibbonacci(n-1) + fibbonacci(n-2));
   }
}


int repeat(char c, int n){
    for(int i = 0 to n){
        print(c);
    }
}

int sum(int n){
    int s = 0;
    while(n >0){
        s = s + n;
        n--;
    }
    return s;
}


int main(){
    print(fibbonacci(6));
    repeat('a', 10);
    print(sum(10));
}

